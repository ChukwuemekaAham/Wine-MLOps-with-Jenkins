pipeline {
    agent { 
        node { 
            label 'MLOpsAgent' 
            customWorkspace '/var/lib/jenkins/workspace/MLOps-pipe' 
        } 
    }

    stages {
        stage('Start FastAPI Server') {
            steps {
                script {
                    // Start the FastAPI server
                    sh 'python3 model_serving.py'
                }
            }
        }
    }

}